{
  "manifest_version": 3,
  "name": "HTML Scraper",
  "version": "1.0",
  "description": "Scrape and send HTML to backend.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://mindler.com/careerlibrary/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
